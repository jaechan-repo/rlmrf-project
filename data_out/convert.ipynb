   "source": [
    "import json\n",
    "\n",
    "INPUT_FILE = \"aokvqa_pred_baseline.json\"\n",
    "OUTPUT_FILE = \"conv_\" + INPUT_FILE\n",
    "\n",
    "with open(INPUT_FILE, 'r') as f:\n",
    "    data = json.load(f)\n",
    "\n",
    "data_converted = {}\n",
    "for line in data:\n",
    "    data_converted.update(line)\n",
    "\n",
    "with open(OUTPUT_FILE, 'w') as f:\n",
    "    json.dump(data_converted, f)"
   ]
